<!-- views/ -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Display Saved Text</title>
</head>
<body>
  <h1>Saved Text</h1>
  <ul>
    <% savedText.forEach((item, index) => { %>
      <li><%= index + 1 %>. <%= item.text %></li>
    <% }); %>
  </ul>
</body>
</html>
